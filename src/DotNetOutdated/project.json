{
  "packOptions": {
    "tags": [ "dotnet", "nuget", "dependencies", "outdated" ],
    "owners": [ "oenning" ]
  },
  "version": "1.0.0-*",
  "description": "Command line tool to check for outdated DotNet Core dependencies.",
  "title": "DotNet Outdated",
  "authors": [ "Guilherme Oenning" ],
  "buildOptions": {
    "outputName": "dotnet-outdated",
    "warningsAsErrors": true,
    "emitEntryPoint": true
  },
  "dependencies": {
    "NuGet.Versioning": "3.5.0-beta2-1484",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      }
    }
  }
}